{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/client",
        "outputDirectory": "dist/client"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/.*", "dest": "/api" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
